<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <solid android:color="@android:color/transparent" />
            <stroke
                android:dashWidth="5dp"
                android:width="3dp"
                android:color="@color/primaryColor" />
            <padding
                android:bottom="15dp"
                android:left="15dp"
                android:right="15dp"
                android:top="15dp" />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
</layer-list>